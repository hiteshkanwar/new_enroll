<%= link_to "Back","#", class: "back-people-page" %>
<% if @employee.employee_family.census_dependents.present? %>
  <%= render 'people/landing_pages/personal' %>
  <%= render 'people/landing_pages/family_members' %>
<% else %>
  <%= render 'people/plans_page' %>
<% end %>
<script>
$('.back-people-page').click(function() {
    $('#person_new').show();
    $('#person_landing').hide();
  })
</script>